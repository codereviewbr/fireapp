    "version": "0.2.0",
    "configurations": [
        {
            "name": "fireapp",
            "request": "launch",
            "type": "dart",
            "toolArgs": [
                "--dart-define",
                "FIREBASE_API_KEY_ANDROID=MY_VALUE",
                "--dart-define",
                "FIREBASE_API_KEY_IOS=MY_VALUE",
            ],
        },
        {
            "name": "fireapp (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "fireapp (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}